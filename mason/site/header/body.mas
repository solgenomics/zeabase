<script defer="defer">


  jQuery(document).ready( function() { 
    jQuery('#working').dialog( { 
      height: 100,
      width:  50,
      modal: true,
      autoOpen: false,
      closeOnEscape: false,
      open: function() { jQuery(this).closest('.ui-dialog').find('.ui-dialog-titlebar-close').hide(); },
      title: 'Working...'
    });

  jQuery('#login_window').dialog( { 
    height: 180,
    width: 300,
    modal: true,
    title: 'Login',
    autoOpen: false,
    buttons: { 
      "Login" : 
        function() { 
          login();
        },
      "Cancel": 
        function() { 
          jQuery('#login_window').dialog("close");
        }
    }
     
   
  });


    setUpLists();
  });

</script>






